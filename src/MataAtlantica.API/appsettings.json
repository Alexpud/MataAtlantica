{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "Default": "%s ENV_CONNECTION_STRINGS_DEFAULT"
  },
  "Files": {
    "UseLocalStorage": "%b ENV_FILES_USE_LOCAL_STORAGE",
    "UseBlobStorage": "%b ENV_FILES_USE_BLOB_STORAGE"
  }
}
